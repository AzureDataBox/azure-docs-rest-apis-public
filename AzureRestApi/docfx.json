{
  "build": {
    "content": [
      {
        "files": [
          "swagger/*.json",
          "*.md"
        ],
        "exclude": [
          "**/obj/**",
          "azure/api/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "azure/api/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "swagger/*.md"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "_mockServerUrl": "https://apiexproxy.azurewebsites.net/svc",
      "_mockServerAuthorization": "Bearer {token:https://graph.windows.net/}"
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "azure/api"
  }
}