{
  "build": {
    "content": [
      {
        "files": [
          "api/**/**.json",
          "api/index.md",
          "api/TOC.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/obj/**",
          "azure/api/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "azure/api/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "overwrite/*.md"
        ],
        "exclude": [
          "api/index.md",
          "api/TOC.md"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata": {
      "_mockServerUrl": "https://apiexproxy.azurewebsites.net/svc",
      "_mockServerAuthorization": "Bearer {token:https://graph.windows.net/}",
      "breadcrumb_path": "/azure/api/toc.json"
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "azure/api"
  }
}
